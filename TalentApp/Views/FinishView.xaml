<UserControl x:Class="JobSeekerApp.Views.FinishView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Height="300" Width="400">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Registration Complete" FontSize="24" Margin="0,0,0,20"/>
            <TextBlock Text="Thank you for registering!" FontSize="18" Margin="0,0,0,10"/>
            <Button Content="View Registered Users" Width="200" Click="ViewUsersButton_Click" Margin="0,20,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>

